#include <iostream>
#include <iomanip> //для std::setw, чтобы выравнивать вывод
using namespace std;

int main() {
    const int size = 10; //размер таблицы (от 1 до 10)

    //вывод заголовков столбцов
    cout << "   |"; //отступ для выравнивания
    for (int i = 1; i <= size; ++i) {
        cout << setw(4) << i; //выравнивание по правому краю
    }
    cout << endl;

    //разделительная линия
    cout << "----+";
    for (int i = 1; i <= size; ++i) {
        cout << "----"; //длина прямоугольников
    }
    cout << endl;

    //основной цикл для печати строк таблицы
    for (int i = 1; i <= size; ++i) {
        cout << setw(4) << i << " |"; //отступ для выравнивания строки

        //вложенный цикл для печати значений
        for (int j = 1; j <= size; ++j) {
            cout << setw(4) << (i * j); //печать произведения i и j
        }

        cout << endl; //переход на следующую строку
    }

    return 0; //окончание программы
}

 
